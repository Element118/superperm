@article {AshlockTillotson,
    author       = {Ashlock, Daniel A. and Tillotson, Jenett},
    title        = {Construction of small superpermutations and minimal injective superstrings},
    year         = {1993},
    journal      = {Congressus Numerantium},
    volume       = {93},
    issn         = {0384-9864},
    pages        = {91--98},
    publisher    = {Utilitas Mathematica Publishing Inc., Winnipeg},
    abstract     = {Summary: A superpermutation is a string over an alphabet $\cal A$ that contains every permutation of the elements of $\cal A$ as a contiguous substring. In this paper, we present a recursive construction for a very small superpermutation on the alphabet ${\cal A}= \{1,2,\dots,n\}$. We also treat the case where every string of length $k< n$ with no repeated characters is to appear as a contiguous substring. Such a string is called an injective superstring on strings of length $k$ over $\cal A$.},
    identifier   = {00617582},
}

